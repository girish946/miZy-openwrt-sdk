#!/bin/bash

## hyphop ##

. ./,config 

feeds_list="
    fluidsynth
    libgme
    busybox-mizy
    libmodplug
    opus-tools
#   vorbis-tools
    libusb-1.0
    sunxi-tools
    markdown
    html2text
"


$test $feeds update -i

next_ignore=

for f in $feeds_list; do

    case $f in
	\#)
	next_ignore=1
	;;
	\#*)
	;;
	*)
	[ "$next_ignore" ] || { 
	    echo "[i] install feed $f"
	    $test $feeds install $f
	}
	next_ignore=
    esac 

done

